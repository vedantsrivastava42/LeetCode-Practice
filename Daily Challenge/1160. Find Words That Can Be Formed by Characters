//Not Optimised code. Optimised Code will use Hashmap.
class Solution {
    public int countCharacters(String[] words, String chars) {
        String w = new String("");
        int result=0;
        int[] arr1 = new int[26];
        for(int i=0;i<chars.length();i++){
           int num=chars.charAt(i)-97;
           arr1[num]++;
        }
        for(int i=0;i<words.length;i++){
          w=words[i];
        int[] arr2 = new int[26];
        for(int j=0;j<w.length();j++){
            int num=w.charAt(j)-97;
            arr2[num]++;
        }
        int flag=0;
        for(int k=0;k<26;k++){
            if(arr2[k]>arr1[k]){
                flag=1;
                break;
            }
        }
           if(flag==0)
            result=result+w.length();

    }
    return result;
}
}
